

<div class="personalPage">
  <div class="container-fluid rounded" id="searchbar">
    <div class="row">
      <div class="col-md-2"></div>
      <div class="col-md-8">
        <h3>Search recipes:</h3>
        <form id="search">
          <div class="input-group">
            {{!-- <label for="searchRecipe">Search Recipe</label> --}}
            <input type="input" class="form-control recipe" id="searchRecipe" placeholder="fried chicken">
            <span class="input-group-btn">
              <button type="submit" class="btn">Submit</button>
            </span>
          </div>
        </form>
      </div>
    </div>
  </div>
  <!--carousel-->
  <div class="container text-center my-3">
    <div id="recipeCarousel" class="carousel slide w-100" data-ride="carousel">
      <div class="carousel-inner w-100" role="listbox">
        <div class="carousel-item row no-gutters active slide1">

        </div>
        <div class="carousel-item row no-gutters slide2">

        </div>
      </div>
      <a class="carousel-control-prev prevResult" href="#recipeCarousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next nextResult" href="#recipeCarousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>
  <div class="tableHolder col-md-6">
    <table class="table" bgcolor="#00FF00">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Favorite Recipe</th>
          <th scope="col">Calories</th>
          <th scope="col">Delete</th>
        </tr>
      </thead>
      <tbody class="fav-container">

      </tbody>
    </table>
  </div>
</div>
